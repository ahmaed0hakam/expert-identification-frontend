{
  "name": "expert-identification-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:prod": "ng build --configuration production",
    "cap:add": "npx cap add",
    "cap:sync": "npx cap sync",
    "cap:open": "npx cap open",
    "cap:run": "npx cap run",
    "android:build": "npm run build:prod && npx cap sync android",
    "android:build:debug": "npm run build && npx cap sync android",
    "android:open": "npm run android:build && npx cap open android",
    "android:open:debug": "npm run android:build:debug && npx cap open android",
    "android:run": "npm run android:build && npx cap run android",
    "android:run:debug": "npm run android:build:debug && npx cap run android",
    "android:apk": "npm run build:prod && npx cap sync android && cd android && ./gradlew assembleRelease",
    "android:apk:debug": "npm run build && npx cap sync android && cd android && ./gradlew assembleDebug",
    "android:aab": "cd android && ./gradlew bundleRelease",
    "android:aab:debug": "cd android && ./gradlew bundleDebug"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@capacitor/android": "^5.4.0",
    "@capacitor/app": "^5.0.7",
    "@capacitor/browser": "^5.0.7",
    "@capacitor/camera": "^5.0.7",
    "@capacitor/core": "^5.4.0",
    "@capacitor/filesystem": "^5.2.2",
    "@capacitor/haptics": "^5.0.7",
    "@capacitor/ios": "^5.4.0",
    "@capacitor/keyboard": "^5.0.7",
    "@capacitor/share": "^5.0.7",
    "@capacitor/status-bar": "^5.0.7",
    "@capacitor-community/file-opener": "^1.0.5",
    "@firebase/app": "^0.9.0",
    "@firebase/auth": "^1.4.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "firebase": "^10.7.0",
    "heic2any": "^0.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@capacitor/cli": "^5.4.0",
    "@types/node": "^18.7.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.1.3"
  }
}
